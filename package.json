{
  "name": "parser",
  "version": "1.0.0",
  "description": "Parse pipe, comma, or space delimited inputs",
  "main": "dist/index.js",
  "scripts": {
    "start": "node ./parse.js",
    "cli": "NODE_ENV=production node ./cli.js",
    "server": "NODE_ENV=production node ./server.js",
    "dev": "NODE_ENV=development nodemon ./server.js",
    "build": "./node_modules/.bin/webpack",
    "test": "NODE_ENV=test ./node_modules/.bin/mocha --compilers js:babel-register --recursive",
    "postinstall": "echo 'bundling the app' && npm run build"
  },
  "keywords": [
    "parse",
    "delimiter",
    "comma",
    "pipe",
    "space"
  ],
  "devDependencies": {
    "babel-polyfill": "^6.20.0",
    "babel-register": "^6.18.0",
    "co-supertest": "0.0.10",
    "jsdom": "^2.0.0",
    "mocha": "^2.1.0",
    "mocha-generators": "^1.2.0",
    "supertest": "^1.0"
  },
  "dependencies": {
    "babel-core": "^6.21.0",
    "babel-loader": "^6.2.10",
    "babel-plugin-transform-object-rest-spread": "^6.20.2",
    "babel-plugin-webpack-alias": "^2.1.2",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-es2017": "^6.16.0",
    "chai": "^3.5.0",
    "json-format": "^1.0.0",
    "json-loader": "^0.5.4",
    "koa": "^1.2.4",
    "koa-body": "^1.6.0",
    "koa-json": "^1.1.3",
    "koa-router": "^5.3.0",
    "lodash": "^4.17.4",
    "nodemon": "^1.11.0",
    "posix-argv-parser": "^2.0.0",
    "webpack": "^1.14.0"
  },
  "author": "Jack Ofnotrade <jooakley@gmail.com> (http://github.com/joaker)",
  "license": "ISC"
}
